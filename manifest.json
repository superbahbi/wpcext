{
	"name": "Watch People Code",
	"version": "0.1",
	"manifest_version": 2,
	"description": "WPC list of live streamers",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon128.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"http://watchpeoplecode.com/json"
	],
	"background": {
		"page": "index.html"
	}
}